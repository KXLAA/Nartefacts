{
  "name": "nartefacts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n NEXT,TS -c magenta,cyan \"next dev\" \"yarn type-check --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "format": "prettier --write .",
    "prepare": "simple-git-hooks",
    "postinstall": "prisma generate",
    "studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "clean": "rm -rf .next && rm -rf node_modules"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.272.0",
    "@prisma/client": "^4.11.0",
    "@radix-ui/react-navigation-menu": "^1.1.1",
    "@radix-ui/react-progress": "^1.0.1",
    "@radix-ui/react-tooltip": "^1.0.4",
    "@stitches/react": "^1.2.8",
    "@tanstack/react-query": "^4.24.10",
    "@trpc/client": "^10.12.0",
    "@trpc/next": "^10.12.0",
    "@trpc/react-query": "^10.12.0",
    "@trpc/server": "^10.12.0",
    "aws-sdk": "^2.1181.0",
    "clsx": "^1.2.1",
    "colorthief": "^2.4.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.0.1",
    "hex-color-to-color-name": "^1.0.2",
    "lucide-react": "^0.122.0",
    "nanoid": "^4.0.1",
    "next": "13.2.1",
    "next-s3-upload": "^0.2.1",
    "next-seo": "^5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-intersection-observer": "^9.4.3",
    "react-use-measure": "^2.1.1",
    "superjson": "^1.12.2",
    "tailwind-merge": "^1.10.0",
    "tailwind-variants": "^0.0.30",
    "typescript": "4.9.5",
    "unique-names-generator": "^4.7.1",
    "use-eye-dropper": "^1.4.3",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.23.1",
    "eslint-config-next": "^13.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.2.7",
    "tailwindcss-animate": "^1.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "./app/**/*.{js,ts,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ],
    "./pages/**/*.{js,ts,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ],
    "./components/**/*.{js,ts,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  }
}
