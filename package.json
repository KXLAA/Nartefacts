{
  "name": "nartefacts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n NEXT,TS -c magenta,cyan \"next dev\" \"yarn ts --watch\"",
    "type-check": "tsc",
    "ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"  --max-warnings=0",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "jest --maxWorkers=50% -u",
    "test:watch": "jest --watch --maxWorkers=25%",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "seed": "npx prisma db seed",
    "studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "gen": "graphql-codegen --config codegen.yml",
    "postinstall": "prisma generate",
    "prepare": "husky install && chmod ug+x .husky/*",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test dev 3000 cypress:open",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "ci": "start-server-and-test start http://localhost:3000"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@formkit/auto-animate": "^1.0.0-beta.1",
    "@hookform/resolvers": "^2.8.8",
    "@prisma/client": "^3.14.0",
    "@radix-ui/react-dialog": "^0.1.7",
    "@radix-ui/react-select": "^1.0.0",
    "@stitches/react": "^1.2.8",
    "@tanstack/react-table": "^8.5.2",
    "apollo-server-micro": "^3.6.4",
    "argon2": "^0.28.5",
    "aws-sdk": "^2.1286.0",
    "colorthief": "^2.3.2",
    "concurrently": "^7.3.0",
    "graphql": "^16.5.0",
    "hex-color-to-color-name": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash.isequal": "^4.5.0",
    "lucide-react": "^0.88.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "nanoid": "^3.3.4",
    "next": "12.2.5",
    "next-auth": "^4.10.2",
    "next-pwa": "^5.4.4",
    "next-s3-upload": "^0.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^12.0.5",
    "react-hook-form": "^7.34.2",
    "react-horizontal-scrolling-menu": "^3.1.1",
    "react-hot-toast": "^2.2.0",
    "react-loader-spinner": "^6.0.0-0",
    "react-use": "^17.4.0",
    "yup": "^0.32.11",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^17.1.0",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@graphql-codegen/typescript-resolvers": "^2.6.1",
    "@next/bundle-analyzer": "^12.2.3",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/react": "^6.4.14",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.0",
    "@types/lodash.isequal": "^4.5.5",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "18.7.8",
    "@types/react": "^17.0.38",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "cy-verify-downloads": "^0.1.8",
    "cypress": "^9.6.1",
    "cypress-file-upload": "^5.0.8",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.22.0",
    "eslint-config-next": "^12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": ">=7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-transform-css": "^4.0.0",
    "lint-staged": ">=12.3.2",
    "plop": "^3.0.5",
    "prettier": "2.7.1",
    "prisma": "^3.14.0",
    "sass": "^1.54.5",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
