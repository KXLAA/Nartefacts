generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

type AlbumsArtist {
    name     String
    photoURL String
}

type AlbumsUrls {
    apple   String
    spotify String
}

model albums {
    id          String       @id @default(auto()) @map("_id") @db.ObjectId
    v           Int          @map("__v")
    albumArt    String
    artist      AlbumsArtist
    colors      String[]
    likeCount   Int
    title       String
    type        String
    urls        AlbumsUrls
    releaseDate DateTime?
}

model analytics {
    id                String @id @default(auto()) @map("_id") @db.ObjectId
    generatedPalettes Int
}

model palettes {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    v         Int      @map("__v")
    imageUrl  String
    createdAt DateTime @default(now())
    palette   String[]
    title     String
}

